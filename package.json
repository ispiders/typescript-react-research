{
    "name": "ts-react-redux-research",
    "version": "2.0.0",
    "private": true,
    "description": "",
    "main": "index.tsx",
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
        "dev": "cross-env NODE_ENV=development webpack --watch --colors --config ./webpack.config.js",
        "start": "cross-env NODE_ENV=development webpack --watch --colors --config ./webpack.config.js",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "test": "jest"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "-test\\.ts$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json"
        ],
        "moduleNameMapper": {
            "^\\$src(.*)$": "<rootDir>/src$1"
        },
        "globals": {
            "ts-jest": {
                "tsConfitFile": "<rootDir>/tsconfig.json"
            }
        }
    },
    "author": "Kyle Young",
    "license": "ISC",
    "dependencies": {
        "jquery": "^1.12.4",
        "react": "^16.1.1",
        "react-dom": "^16.1.1",
        "react-redux": "^5.0.6",
        "react-router-dom": "^4.2.2"
    },
    "devDependencies": {
        "@types/jest": "^21.1.8",
        "@types/jquery": "^3.2.16",
        "@types/react": "^16.0.25",
        "@types/react-dom": "^16.0.3",
        "@types/react-redux": "^5.0.13",
        "@types/react-router-dom": "^4.2.2",
        "awesome-typescript-loader": "^3.4.0",
        "cross-env": "^4.0.0",
        "css-loader": "^0.25.0",
        "file-loader": "^0.9.0",
        "jest": "^21.2.1",
        "source-map-loader": "^0.2.3",
        "style-loader": "^0.13.1",
        "ts-jest": "^21.2.3",
        "typescript": "^2.6.1",
        "url-loader": "^0.5.7",
        "webpack": "^3.8.1",
        "webpack-dev-server": "^1.16.2"
    }
}
